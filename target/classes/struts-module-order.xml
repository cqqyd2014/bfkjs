<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

	<package name="bfkjs-order" namespace="/order" extends="struts-default">

		 <interceptors>  
            <interceptor name="authorityInterceptor" class="com.cqqyd2014.interceptor.AuthorityInterceptor"></interceptor>  
            <!-- 一个拦截器栈中可以定义多个拦截器 -->  
            <interceptor-stack name="authorityStack">  
                <interceptor-ref name="authorityInterceptor" />  
                <interceptor-ref name="defaultStack" />  
            </interceptor-stack>  
        </interceptors>  
        <!--全局结果处理  -->  
        <global-results>  
            <result name="login">../portal/qyd</result>
            <result name="ajax_login">../portal/qyd</result>  
        </global-results>  

	</package>
</struts>    
